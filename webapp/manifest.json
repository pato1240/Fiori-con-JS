{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.logali.invoices",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.2",
            "toolsId": "d7511be4-4e14-4e7a-8e14-f36b3ff054fb"
        },
        "dataSources": {
            "myNorthwind": {
                "type": "OData",
                "uri": "/v2/northwind/northwind.svc/",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.logali.invoices.i18n.i18n"
                }
            },
            "invoices": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/Invoices.json"
            },
            "northwind": {
                "dataSource": "myNorthwind",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "com.logali.invoices.view",
                "async": true,
                "viewPath": "com.logali.invoices.view"
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": "",
                    "target": [
                        "TargetOverview"
                    ]
                },
                {
                    "name": "Details",
                    "pattern": "details/{invoicePath}",
                    "target": [
                        "TargetDetails"
                    ]
                }
            ],
            "targets": {
                "TargetOverview": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Overview",
                    "viewName": "Overview"
                },
                "TargetDetails": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Details",
                    "viewName": "Details"
                    }
                
            }
        },
        "rootView": {
            "viewName": "com.logali.invoices.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}